{
    "default_server_config": {
        "m.homeserver": {
            "base_url": "https://{{ setups.matrix.config.synapse_domain }}",
            "server_name": "{{ setups.matrix.config.server_name }}"
        },
        "m.identity_server": {
            "base_url": "https://vector.im"
        }
    },
    "disable_custom_urls": true,
    "disable_guests": true,
